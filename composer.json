        "ext-ctype": "*",
        "ext-iconv": "*",
        "fproject/php-jwt": "^4.0",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "4.4.*",
        "symfony/yaml": "4.4.*"
    },
    "require-dev": {
    },
    "autoload": {
